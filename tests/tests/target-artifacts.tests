SECTION 'default variant'

TEST-CASE 'one variant, one artifact'
	VARIANT 'set 1'
		DB__ARTIFACTS_target_1=('one')
		target-artifacts 'target_1'
		CHECK [ "${#TARGET_ARTIFACTS_RETURN__ARTIFACTS[@]}" == '1' ]
		CHECK [ "${TARGET_ARTIFACTS_RETURN__ARTIFACTS[0]}" == 'one' ]

