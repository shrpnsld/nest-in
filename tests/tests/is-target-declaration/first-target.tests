source $(SAMPLE nest-in)

TEST-CASE 'plain'
	CHECK is-target-declaration '.first'

TEST-CASE '.first / [requirement]'
	CHECK is-target-declaration '.first / [one]'
	CHECK is-target-declaration '.first / [three]'
	CHECK is-target-declaration '.first / [one] [three]'

TEST-CASE '.first / [requirement:specifier]'
	CHECK is-target-declaration '.first / [one:two]'
	CHECK is-target-declaration '.first / [three:four]'
	CHECK is-target-declaration '.first / [one:two] [three:four]'

TEST-CASE '.first{SPACING}'
	CHECK is-target-declaration $'.first '
	CHECK is-target-declaration $'.first\t'
	CHECK is-target-declaration $'.first  '
	CHECK is-target-declaration $'.first \t'
	CHECK is-target-declaration $'.first\t '
	CHECK is-target-declaration $'.first\t\t'
	CHECK is-target-declaration $'.first\t  '
	CHECK is-target-declaration $'.first\t \t'
	CHECK is-target-declaration $'.first\t\t '

TEST-CASE '.first{SPACING}/ [requirement]'
	CHECK is-target-declaration $'.first\t/ [one]'
	CHECK is-target-declaration $'.first  / [one]'
	CHECK is-target-declaration $'.first \t/ [one]'
	CHECK is-target-declaration $'.first\t / [one]'
	CHECK is-target-declaration $'.first\t\t/ [one]'
	CHECK is-target-declaration $'.first\t  / [one]'
	CHECK is-target-declaration $'.first\t \t/ [one]'
	CHECK is-target-declaration $'.first\t\t / [one]'

TEST-CASE '.first /{SPACING}[requirement]'
	CHECK is-target-declaration $'.first /\t[one]'
	CHECK is-target-declaration $'.first /  [one]'
	CHECK is-target-declaration $'.first / \t[one]'
	CHECK is-target-declaration $'.first /\t [one]'
	CHECK is-target-declaration $'.first /\t\t[one]'
	CHECK is-target-declaration $'.first /\t  [one]'
	CHECK is-target-declaration $'.first /\t \t[one]'
	CHECK is-target-declaration $'.first /\t\t [one]'

TEST-CASE '.first / [requirement]{SPACING}'
	CHECK is-target-declaration $'.first / [one] '
	CHECK is-target-declaration $'.first / [one]\t'
	CHECK is-target-declaration $'.first / [one]  '
	CHECK is-target-declaration $'.first / [one] \t'
	CHECK is-target-declaration $'.first / [one]\t '
	CHECK is-target-declaration $'.first / [one]\t\t'
	CHECK is-target-declaration $'.first / [one]\t  '
	CHECK is-target-declaration $'.first / [one]\t \t'
	CHECK is-target-declaration $'.first / [one]\t\t '

TEST-CASE '.first / [requirement]{SPACING}[requirement]'
	CHECK is-target-declaration $'.first / [one] [two]'
	CHECK is-target-declaration $'.first / [one]\t[two]'
	CHECK is-target-declaration $'.first / [one]  [two]'
	CHECK is-target-declaration $'.first / [one] \t[two]'
	CHECK is-target-declaration $'.first / [one]\t [two]'
	CHECK is-target-declaration $'.first / [one]\t\t[two]'
	CHECK is-target-declaration $'.first / [one]\t  [two]'
	CHECK is-target-declaration $'.first / [one]\t \t[two]'
	CHECK is-target-declaration $'.first / [one]\t\t [two]'

