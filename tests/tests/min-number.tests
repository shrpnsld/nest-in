source $(SAMPLE nest-in)

TEST-CASE 'with [first] < [second]'
	min-number 1 2
	CHECK [ $? == 1 ]

	min-number 2 3
	CHECK [ $? == 2 ]

TEST-CASE 'with [first] > [second]'
	min-number 2 1
	CHECK [ $? == 1 ]

	min-number 3 2
	CHECK [ $? == 2 ]

TEST-CASE 'with equal arguments'
	min-number 1 1
	CHECK [ $? == 1 ]

	min-number 2 2
	CHECK [ $? == 2 ]

